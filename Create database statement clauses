psql -U postgres -h localhost

create database nutritrack;
\l

\c nutritrack;

CREATE TABLE meal (
    mealid     BIGSERIAL NOT NULL PRIMARY KEY,
    foodid    BIGINT,
    amount float
);

CREATE TABLE Food (
foodid BIGSERIAL NOT NULL PRIMARY KEY,
manufacturer VARCHAR(100),
description VARCHAR(100),
energy float,
carbs float,
sugar float,
fiber float,
fat float,
saturated float,
unsaturated float,
protein float,
salt float
);

CREATE TABLE NutritrackUser (
userid BIGSERIAL NOT NULL PRIMARY KEY,
email varchar (100),
password varchar (100),
targetenergy float,
targetprotein float,
targetfat float,
targetcarbs float,
targetweight float,
targetrate float,
historyid BIGINT,
days DATE
); 
