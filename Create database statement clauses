psql -U postgres -h localhost

create database nutritrack;
\l

\c nutritrack;

REATE TABLE meal (
    mealid     BIGSERIAL NOT NULL PRIMARY KEY,
    food    BiGINT,
    amount float
);

CREATE TABLE Food (
foodid BIGSERIAL NOT NULL PRIMARY KEY,
manufacturer VARCHAR(100),
description VARCHAR(100),
energy float,
carbs float,
sugar float,
fiber float,
fat float,
saturated float,
unsaturated float,
protein float,
salt float
);

