const {Client} = require('pg')
const client = new Client({
	user: "postgres",
	password: "",
	host: "localhost",
	port: 5432,
	database: "nutritrack"
})

client.connect()
.then(() => console.log("connected succesfully"))
.then(() => client.query("select * from meal")  )
.then( results => console.table(results.rows) )
.catch(e => console.log(e))
.finally(() => client.end())
